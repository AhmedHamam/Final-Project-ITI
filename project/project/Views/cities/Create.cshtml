@model project.Models.city

@{
    ViewBag.Title = "إضافة مدينة";
    Layout = "~/Views/Shared/_adminMainLayout.cshtml";
}
<div class="col-md-3"></div>
<div class="col-md-6">
    <h2 class="text-center btn-success">إضافة مدينة</h2>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
    <div class="form-horizontal">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            
            <div class="col-md-10">
                <h6 class="text-center">اسم المدينة</h6>
                @Html.EditorFor(model => model.name, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.name, "", new { @class = "text-danger" })

                <br />

            </div>
        </div>
        <div class="form-group">
            <div class="col-md-10">
                <h6 class="text-center">اسم المحافظة</h6>
                @Html.DropDownList("gov_id", null, htmlAttributes: new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.gov_id, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="حفظ" class="btn btn-primary" />
                <br /><br />
                @Html.ActionLink("العودة الى القائمة السابقة", "Index", null, new { @class = "text-center" })
            </div>
        </div>
    </div>
    }
</div>
<div class="col-md-6">
</div>
   

      
