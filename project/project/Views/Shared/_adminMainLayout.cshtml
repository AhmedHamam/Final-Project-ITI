                    <a class="nav-link" href="#" style="color:white;">الصفحه الرئيسية<span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item active">
                    <a class="nav-link" href="#" style="color:white;">تسجيل الخروج<span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#" style="color:white;">تسجيل الخروج</a>
                </li>
            </ul>
        </div>
    </nav>
    <div class="container">
        <div class="row">
            @RenderBody()
        </div>
    </div>
    <footer class="ftco-footer ftco-section" style="padding-top:5%; background:rgb(37, 163, 153); opacity:2;">
        <div class="container">
            <div class="row">
                <div class="col-md-12 text-center">

                    <p>
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
